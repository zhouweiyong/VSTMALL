    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Dialog_bocop">
        <item name="android:windowBackground">@color/bocop_dialog_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
    <style name="Loading_Dialog_trans" parent="Dialog_bocop">
        <item name="android:windowBackground">@color/color_transparent</item>
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="main_tab_icon">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="main_tab_tv">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/color_selector_main_tab</item>
    </style>
</resources>